{
    "name": "slim-skeleton-app",
    "require": {
        "php": ">=5.4",
	"slim/slim": "2.*",
        "doctrine/cache": "*",
        "icanboogie/inflector": "*"
    },
    "require-dev": {
	"squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.*",
	"phpunit/phpunit": "4.*",
        "there4/slim-test-helpers": "@stable",
        "phing/phing": "2.*"
    },
    "autoload": {
	"psr-4": {
	    "Slimish\\": "src/application/library/"
	}
    },
    "autoload-dev": {
        "psr-4": {
            "SlimishTest\\": "tests/library/"
        }
    }
}
